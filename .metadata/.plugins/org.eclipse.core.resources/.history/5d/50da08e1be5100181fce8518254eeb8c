package client;

import java.rmi.RemoteException;
import java.rmi.registry.LocateRegistry;

import server.FrueherkennungsIF;
import server.Roentgenbild;

public class UniklinikumRegensburg {

	public static void main(String[] args) {
		try {
			FrueherkennungsIF server = (FrueherkennungsIF) LocateRegistry.getRegistry("localhost", 1099);
			server.analysiere(new Roentgenbild());
			
		} catch (RemoteException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
		

	}

}
