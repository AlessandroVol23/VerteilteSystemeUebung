package de.othr.vs;

import java.io.IOException;
import java.net.ServerSocket;

public class Server {

	public static void main(String[] args) {
		ServerSocket server = null;
		int port=0;
		if (args.length > 0) {
			port = args[0];
		}
		try {
			server = new ServerSocket(8080);
			server.close();

			
			while (true) {
				
			}
		} catch (IOException e) {
			e.printStackTrace();
		}
	}

}
