package ueubung01;

public class Simluation{
	
	public static void main(String[] args) throws InterruptedException {
		
		Parkhaus parkhaus = new Parkhaus(10);
		
		for(int i=1; i<=20; i++) {
			Auto a =new Auto("R-FH " + i, parkhaus);
			a.setDaemon(true);
			a.start();
		}
		
		Thread.sleep(30000);
		System.out.println("Ende der Simulation!");
	}

}
