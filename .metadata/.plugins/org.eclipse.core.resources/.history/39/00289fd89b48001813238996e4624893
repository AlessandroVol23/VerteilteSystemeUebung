package de.othr.vs;

import java.io.IOException;
import java.net.ServerSocket;

public class Server {

	public static void main(String[] args) {
		ServerSocket server = null;
		
		 for (String s: args) {
	            System.out.println(s);
	        }
		try {
			server = new ServerSocket(8080);

			while (true) {
				
			}
		} catch (IOException e) {
			e.printStackTrace();
		}
		server.close();
	}

}
