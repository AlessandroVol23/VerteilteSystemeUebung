		fhThread.setDaemon(true);
		fhThread.start();

		Kunden kunde1 = new Kunden(auslieferungslager);
		Kunden kunde2 = new Kunden(auslieferungslager);
		Kunden kunde3 = new Kunden(auslieferungslager);
		Kunden kunde4 = new Kunden(auslieferungslager);
		Kunden kunde5 = new Kunden(auslieferungslager);
		


	}

